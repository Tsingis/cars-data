{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./config/vite.config.ts --open",
    "test": "npx playwright test --config=tests/playwright.config.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --config ./config/.eslintrc.cjs",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --config ./config/.eslintrc.cjs --fix",
    "setup-test": "npx playwright install --with-deps",
    "build": "tsc -b ./config/tsconfig.json && vite --config ./config/vite.config.ts build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "chart.js": "^4.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "typescript": "^5.5.4",
    "vite": "^5.3.4"
  }
}
