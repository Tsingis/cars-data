{
  "name": "cars-data",
  "type": "module",
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "predev": "node check-packages.js",
    "dev": "vite --config ./vite.config.ts --open",
    "dev:check": "vite --config ./vite.config.ts --open -- --use-checker",
    "preview": "vite preview --config ./vite.config.ts",
    "http": "http-server ../data/misc --cors -p 8000",
    "playwright:setup": "playwright install --with-deps",
    "playwright": "playwright test --config=playwright.config.ts",
    "playwright:update": "playwright test --config=playwright.config.ts --update-snapshots",
    "cypress:setup": "concurrently -k \"pnpm run preview\" \"pnpm run http\"",
    "cypress": "start-server-and-test 'pnpm run cypress:setup' \"http://localhost:3000|http://localhost:8000\" 'cypress run'",
    "vitest": "vitest --config vitest.config.ts",
    "audit": "pnpm audit --audit-level high",
    "eslint": "eslint . --config ./eslint.config.js",
    "eslint:fix": "eslint . --config ./eslint.config.js --fix",
    "lint": "biome lint --diagnostic-level=warn .",
    "lint:fix": "biome lint --diagnostic-level=warn --write .",
    "format": "biome format --diagnostic-level=warn .",
    "format:fix": "biome format --diagnostic-level=warn --write .",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "build": "tsc -b ./tsconfig.json && vite --config ./vite.config.ts build",
    "prepare": "cd .. && husky app/.husky"
  },
  "minimumReleaseAge": 60,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "chart.js": "^4.5.1",
    "chartjs-chart-treemap": "^3.1.0",
    "i18next": "^25.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.1.2",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.4.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "start-server-and-test": "^2.1.2",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.1.19",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^3.2.4"
  }
}
