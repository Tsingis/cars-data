{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node check-packages.js",
    "dev": "vite --config ./vite.config.ts --open",
    "dev:check": "vite --config ./vite.config.ts --open -- --use-checker",
    "preview": "vite preview --config ./vite.config.ts",
    "http": "http-server ../data --cors -p 8000",
    "playwright:setup": "playwright install --with-deps",
    "playwright": "playwright test --config=playwright.config.ts",
    "playwright:update": "playwright test --config=playwright.config.ts --update-snapshots",
    "cypress:setup": "concurrently -k \"pnpm run preview\" \"pnpm run http\"",
    "cypress": "start-server-and-test 'pnpm run cypress:setup' \"http://localhost:3000|http://localhost:8000\" 'cypress run'",
    "vitest": "vitest --config vitest.config.ts",
    "audit": "pnpm audit --audit-level high",
    "eslint": "eslint . --config ./eslint.config.js",
    "eslint:fix": "eslint . --config ./eslint.config.js --fix",
    "lint": "biome lint --diagnostic-level=warn .",
    "lint:fix": "biome lint --diagnostic-level=warn --write .",
    "format": "biome format --diagnostic-level=warn .",
    "format:fix": "biome format --diagnostic-level=warn --write .",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "build": "tsc -b ./tsconfig.json && vite --config ./vite.config.ts build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "chart.js": "^4.5.0",
    "chartjs-chart-treemap": "^3.1.0",
    "i18next": "^25.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.6.1",
    "react-router": "^7.7.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.4",
    "eslint": "^9.32.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-plugin-checker": "^0.10.2",
    "vitest": "^3.2.4"
  }
}
