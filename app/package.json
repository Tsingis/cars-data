{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./vite.config.ts --open",
    "dev:check": "vite --config ./vite.config.ts --open -- --use-checker",
    "http": "python ../util/http_server.py",
    "setup:e2e": "npx playwright install --with-deps",
    "test:e2e": "npx playwright test --config=playwright.config.ts",
    "test:components": "npx vitest --config vitest.config.ts",
    "audit": "npm audit --audit-level high",
    "lint": "npx eslint . --config ./eslint.config.js",
    "lint:fix": "npx eslint . --config ./eslint.config.js --fix",
    "prettier": "npx prettier --check '**/*.{js,jsx,ts,tsx,css,html}' --config ../.prettierrc.json",
    "prettier:fix": "npx prettier --write '**/*.{js,jsx,ts,tsx,css,html}'",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "build": "tsc -b ./tsconfig.json && vite --config ./vite.config.ts build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "chart.js": "^4.4.9",
    "chartjs-chart-treemap": "^3.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.5.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.0",
    "eslint": "^9.24.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vite-plugin-checker": "^0.9.1",
    "vitest": "^3.1.1"
  }
}
